<Window
    Title="DebugStatus"
    d:DesignHeight="250"
    d:DesignWidth="400"
    mc:Ignorable="d"
    x:Class="InstallyApp.DebugStatus"
    xmlns="https://github.com/avaloniaui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
        <Grid Background="{DynamicResource SecondaryColor}" Name="DebugGrid">
            <ScrollViewer Name="DebugWrapper" />
        </Grid>
    </Grid>
</Window>
